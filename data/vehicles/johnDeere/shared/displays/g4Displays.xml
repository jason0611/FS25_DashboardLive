            <dashboard displayType="EMITTER" valueType="powerTakeOffs.state" node="aPillarDisplay_iconPTOFront_decal" baseColor="0 0 0" emitColor="SHARED_YELLOW2" intensity="0.5" intensityDarkMode="0.4" powerTakeOffIndex="2" groups="MOTOR_ACTIVE"/>
