        <dashboard displayType="SLIDER" valueType="motorized.motorTemperature" node="aPillarDisplay_sliderTemperature_decal" minValueSlider="0" maxValueSlider="150" intensity="0.5" intensityDarkMode="0.2" hideInactive="true" groups="MOTOR_ACTIVE"/>
