            <objectChange node="aPillarDisplay_iconPTOFront_decal" visibilityActive="true" visibilityInactive="false"/>
       </configuration>
        <configuration id="PTO_BACK_ONLY">
            <dashboard displayType="EMITTER" valueType="powerTakeOffs.state" node="aPillarDisplay_iconPTOBack_decal" baseColor="0 0 0" emitColor="SHARED_YELLOW2" intensity="0.5" intensityDarkMode="0.4" powerTakeOffIndex="1" groups="MOTOR_ACTIVE"/>
        </configuration>
    </dashboardCompound>

