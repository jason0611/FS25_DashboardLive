<?xml version="1.0" encoding="utf-8" standalone="no"?>
<dashboardCompounds xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../shared/xml/schema/dashboardCompounds.xsd">
    <dashboardCompound name="FENDT_ONE_DASHBOARD" filename="data/vehicles/fendt/shared/oneDisplays.i3d" node="fendtOneDisplayFront">
        <dashboard displayType="EMITTER" valueType="lights.turnLightLeft" node="iconTurnlightLeft_decal" intensity="1" baseColor="SHARED_WHITE2" groups="MOTOR_ACTIVE"/>
        <dashboard displayType="EMITTER" valueType="lights.turnLightRight" node="iconTurnlightRight_decal" intensity="1" baseColor="SHARED_WHITE2" groups="MOTOR_ACTIVE"/>
        <dashboard displayType="EMITTER" valueType="lights.turnLightAny" node="iconTurnlightTrailer01_decal" intensity="0.2" baseColor="SHARED_BLACK4" groups="MOTOR_ACTIVE FRONT_ATTACHMENT"/>
        <dashboard displayType="EMITTER" valueType="lights.turnLightAny" node="iconTurnlightTrailer02_decal" intensity="0.2" baseColor="SHARED_BLACK4" groups="MOTOR_ACTIVE BACK_ATTACHMENT"/>

        <dashboard displayType="VISIBILITY" valueType="motorized.directionForwardExclusive" node="iconForwardStanding_decal" groups="MOTOR_ACTIVE" />
        <dashboard displayType="VISIBILITY" valueType="motorized.directionForwardExclusive" node="iconForward_decal" groups="MOTOR_ACTIVE" />
        <dashboard displayType="VISIBILITY" valueType="motorized.directionBackward" node="iconBackwardStanding_decal" groups="MOTOR_ACTIVE" />
        <dashboard displayType="VISIBILITY" valueType="motorized.directionBackward" node="iconBackward_decal" groups="MOTOR_ACTIVE" />

        <dashboard displayType="MULTI_STATE" valueType="drivable.movingDirection" node="iconsStanding" groups="MOTOR_ACTIVE">
            <state value="-1" visibility="false"/>
            <state value="0" visibility="true"/>
            <state value="1" visibility="false"/>
        </dashboard>
        <dashboard displayType="MULTI_STATE" valueType="drivable.movingDirection" node="iconsDriving" groups="MOTOR_ACTIVE">
            <state value="-1" visibility="true"/>
            <state value="0" visibility="false"/>
            <state value="1" visibility="true"/>
        </dashboard>

        <dashboard displayType="EMITTER" valueType="dbl.vca" cmd="park" node="iconEngineStart_decal" idleValue="-1" intensity="0.3" groups="MOTOR_ACTIVE" />
        <dashboard displayType="EMITTER" valueType="drivable.cruiseControlActive" node="iconCruiseControl01_decal" inactiveIntensity="0.2" intensity="0.3" inactiveColor="SHARED_WHITE1" emitColor="GREEN" groups="MOTOR_ACTIVE"/>
        <dashboard displayType="EMITTER" valueType="lights.lightState" node="iconDefaultLight01_decal" lightTypes="0" baseColor="SHARED_BLACK4" intensity="0.3" groups="MOTOR_ACTIVE"/>

        <dashboard displayType="TEXT" valueType="motorized.rpm" node="numbersRPM01" textColor="0.4 0.4 0.4 1" textSize="0.0175" fontThickness="0.4" textMask="0000" font="GENERIC" textAlignment="CENTER" groups="MOTOR_ACTIVE"/>
        <dashboard displayType="TEXT" valueType="motorized.speed" node="numbersSpeed01" textColor="0.4 0.4 0.4 1" textSize="0.0175" fontThickness="0.4" textMask="00.0" font="GENERIC" textAlignment="CENTER" groups="MOTOR_ACTIVE"/>

        <dashboard displayType="SLIDER" valueType="motorized.motorTemperature" node="sliderTemperature" minValueSlider="40" maxValueSlider="120" intensity="1" groups="MOTOR_ACTIVE"/>
        <dashboard displayType="SLIDER" valueType="motorized.rpm" node="sliderRPM" minValueSlider="0" maxValueSlider="2500" intensity="1" groups="MOTOR_ACTIVE"/>
        <dashboard displayType="SLIDER" valueType="motorized.speed" node="sliderSpeed01" minValueSlider="0" maxValueSlider="63" intensity="1" groups="MOTOR_ACTIVE"/>

        <dashboard displayType="SLIDER" valueType="fillLevel" fillType="AIR" node="sliderAir" groups="MOTOR_ACTIVE"/>
        <dashboard displayType="SLIDER" valueType="fillLevel" fillType="DEF" node="sliderDef" groups="MOTOR_ACTIVE"/>
        <dashboard displayType="SLIDER" valueType="fillLevel" fillType="DIESEL" node="sliderFuel" groups="MOTOR_ACTIVE"/>

        <dashboard displayType="EMITTER" node="iconForward_decal" baseColor="SHARED_WHITE2" emitColor="FENDT_ORANGE1" groups="MOTOR_ACTIVE"/>
        <dashboard displayType="EMITTER" node="iconForwardStanding_decal" baseColor="SHARED_WHITE2" emitColor="FENDT_ORANGE1" groups="MOTOR_ACTIVE"/>
        <dashboard displayType="EMITTER" node="iconBackward_decal" baseColor="SHARED_WHITE2" emitColor="FENDT_ORANGE1" groups="MOTOR_ACTIVE"/>
        <dashboard displayType="EMITTER" node="iconBackwardStanding_decal" baseColor="SHARED_WHITE2" emitColor="FENDT_ORANGE1" groups="MOTOR_ACTIVE"/>
        <dashboard displayType="EMITTER" node="fendtOneDisplayFront_decal" idleValue="-1" intensity="0.2" groups="MOTOR_ACTIVE" />
        <dashboard displayType="VISIBILITY" node="fendtOneDisplayFrontContent" groups="MOTOR_ACTIVE" />
		
	<!--
	*********DASHBOARD LIVE*********
	-->
		<dashboard displayType="VISIBILITY" node="PTOfront_decal" groups="MOTOR_ACTIVE" />
		<dashboard valueType="dbl.base" cmd="pto" displayType="EMITTER" node="PTOfront_decal" emitColor="ORANGE" joints="2 5" intensity="0.8" groups="MOTOR_ACTIVE" />
		<dashboard displayType="VISIBILITY" node="PTOback_decal" groups="MOTOR_ACTIVE" />
		<dashboard valueType="dbl.base" cmd="pto" displayType="EMITTER" node="PTOback_decal" emitColor="ORANGE" joints="1 3 4" intensity="0.8" groups="MOTOR_ACTIVE" />
		<dashboard displayType="VISIBILITY" node="PTOfrontTractor_decal" groups="MOTOR_ACTIVE" />
		<dashboard valueType="dbl.base" cmd="pto" displayType="EMITTER" node="PTOfrontTractor_decal" emitColor="ORANGE" joints="2 5" intensity="0.8" groups="MOTOR_ACTIVE" />
		<dashboard displayType="VISIBILITY" node="PTObackTractor_decal" groups="MOTOR_ACTIVE" />
		<dashboard valueType="dbl.base" cmd="pto" displayType="EMITTER" node="PTObackTractor_decal" emitColor="ORANGE" joints="1 3 4" intensity="0.8" groups="MOTOR_ACTIVE" />
	<!--
		PTOback_decal
		<dashboard valueType="dbl.vca" cmd="park" displayType="VISIBILITY" node="dbl_Park" groups="MOTOR_ACTIVE" />
		<dashboard valueType="dbl.vca" cmd="park" displayType="EMITTER" node="dbl_Park" emitColor="RED" intensity="0.8" groups="MOTOR_ACTIVE" />
	-->	
		
		
    </dashboardCompound>

    <dashboardCompound name="FENDT_ONE_MAIN" filename="data/vehicles/fendt/shared/oneDisplays.i3d" node="fendtOneDisplaySide">
        <dashboard displayType="EMITTER" valueType="lights.lightState" node="iconDefaultLight02_decal" lightTypes="0" idleValue="0.1" intensity="0.7" groups="MOTOR_ACTIVE"/>
        <dashboard displayType="EMITTER" valueType="lights.lightState" node="iconWorklightFront_decal" lightTypes="2" idleValue="0.1" intensity="0.7" groups="MOTOR_ACTIVE"/>
        <dashboard displayType="EMITTER" valueType="lights.lightState" node="iconWorklightBack_decal" lightTypes="1" idleValue="0.1" intensity="0.7" groups="MOTOR_ACTIVE"/>
        <dashboard displayType="EMITTER" valueType="lights.beaconLight" node="iconBeaconLight_decal" idleValue="0.1" intensity="0.7"/>

        <dashboard displayType="EMITTER" valueType="drivable.cruiseControlActive" node="iconCruiseControl02_decal" inactiveIntensity="0.2" intensity="0.2" inactiveColor="SHARED_WHITE1" emitColor="GREEN" groups="MOTOR_ACTIVE"/>


        <dashboard displayType="ROT" valueType="drivable.cruiseControl" node="iconCruiseControl03" minRot="0 0 0" maxRot="-268 0 0" minValueRot="0" maxValueRot="63" groups="MOTOR_ACTIVE"/>


        <dashboard displayType="TEXT" valueType="enterable.time" node="numbersTime01" textColor="1 1 1 1" textSize="0.0025" textMask="00:00" font="GENERIC" textAlignment="CENTER" emissiveScale="0.5" groups="MOTOR_ACTIVE"/>
        <dashboard displayType="TEXT" valueType="motorized.speed" node="numbersSpeed02" textColor="1 1 1 1" textSize="0.005" textMask="00.0" font="GENERIC" textAlignment="CENTER" emissiveScale="0.5" groups="MOTOR_ACTIVE"/>
        <dashboard displayType="TEXT" valueType="motorized.fuelUsage" node="numbersFuelUsage01" textColor="0.5 0.5 0.5 1" textSize="0.005" textMask="00.0" font="GENERIC" groups="MOTOR_ACTIVE"/>
        <dashboard displayType="TEXT" valueType="drivable.cruiseControl" node="numbersCruiseControl01" textColor="0.5 0.5 0.5 1" textSize="0.005" textMask="00.0" textAlignment="center" textScaleX="0.88" font="GENERIC" groups="MOTOR_ACTIVE"/>
        <dashboard displayType="TEXT" valueType="drivable.cruiseControl" node="numbersCruiseControl02" textColor="0.5 0.5 0.5 1" textSize="0.005" textMask="00.0" textAlignment="center" textScaleX="0.88" font="GENERIC" groups="MOTOR_ACTIVE"/>
        <dashboard displayType="TEXT" valueType="drivable.cruiseControl" node="numbersCruiseControl03" textColor="0.5 0.5 0.5 1" textSize="0.004" textMask="00.0" textScaleX="0.88" font="GENERIC" groups="MOTOR_ACTIVE"/>


        <dashboard displayType="SLIDER" valueType="motorized.speed" node="sliderSpeed02" minValueSlider="0" maxValueSlider="63" groups="MOTOR_ACTIVE"/>


        <dashboard displayType="EMITTER" node="fendtOneDisplaySide_decal" idleValue="-1" intensity="0.2" groups="MOTOR_ACTIVE" />
        <dashboard displayType="VISIBILITY" node="fendtOneDisplaySideContent" groups="MOTOR_ACTIVE" />
    </dashboardCompound>

    <dashboardCompound name="FENDT_ONE_TOP" filename="data/vehicles/fendt/shared/oneDisplays.i3d" node="fendtOneDisplayRoof">
        <dashboard displayType="TEXT" valueType="enterable.time" node="numbersTime02" textColor="1 1 1 1" textSize="0.0025" textMask="00:00" font="GENERIC" textAlignment="CENTER" emissiveScale="0.5" groups="MOTOR_ACTIVE"/>
        <dashboard displayType="TEXT" valueType="motorized.fuelUsage" node="numbersFuelUsage02" textColor="0.3 0.3 0.3 1" textSize="0.006" textMask="00.0" font="GENERIC" groups="MOTOR_ACTIVE"/>
        <dashboard displayType="TEXT" valueType="motorized.fuelUsage" node="numbersFuelUsage03" textColor="0.5 0.5 0.5 1" textSize="0.005" textMask="00.0" font="GENERIC" groups="MOTOR_ACTIVE"/>
        <dashboard displayType="TEXT" valueType="drivable.cruiseControl" node="numbersCruiseControl04" textColor="0.5 0.5 0.5 1" textSize="0.005" textMask="00.0" textAlignment="center" textScaleX="0.88" font="GENERIC" groups="MOTOR_ACTIVE"/>
        <dashboard displayType="EMITTER" valueType="drivable.cruiseControlActive" node="iconCruiseControl03_decal" inactiveIntensity="0.2" intensity="0.2" inactiveColor="SHARED_WHITE1" emitColor="GREEN" groups="MOTOR_ACTIVE"/>


        <dashboard displayType="EMITTER" node="fendtOneDisplayRoof_decal" idleValue="-1" intensity="0.2" groups="MOTOR_ACTIVE" />
        <dashboard displayType="VISIBILITY" node="fendtOneDisplayRoofContent" groups="MOTOR_ACTIVE" />
    </dashboardCompound>

    <i3dMappings>
	<!-- FendtOne Display Front -->
	  <!--Icons-->
        <i3dMapping id="fendtOneDisplayFront" node="0>" />
        <i3dMapping id="fendtOneDisplayFrontContent" node="0>0" />
        <i3dMapping id="iconTurnlightLeft_decal" node="0>0|0|0" />
        <i3dMapping id="iconTurnlightRight_decal" node="0>0|0|1" />
        <i3dMapping id="iconTurnlightTrailer01_decal" node="0>0|0|2" />
        <i3dMapping id="iconTurnlightTrailer02_decal" node="0>0|0|3" />
        <i3dMapping id="iconsStanding" node="0>0|0|4" />
        <i3dMapping id="iconForwardStanding_decal" node="0>0|0|4|0" />
        <i3dMapping id="iconBackwardStanding_decal" node="0>0|0|4|1" />
        <i3dMapping id="iconsDriving" node="0>0|0|5" />
        <i3dMapping id="iconForward_decal" node="0>0|0|5|0" />
        <i3dMapping id="iconBackward_decal" node="0>0|0|5|1" />
        <i3dMapping id="iconEngineStart_decal" node="0>0|0|6" />
        <i3dMapping id="iconCruiseControl01_decal" node="0>0|0|7" />
        <i3dMapping id="iconDefaultLight01_decal" node="0>0|0|8" />
		<!-- PTO -->
		<i3dMapping id="PTOfront_decal" node="0>0|0|9" />
		<i3dMapping id="PTOfrontTractor_decal" node="0>0|0|9|0" />
		<i3dMapping id="PTOback_decal" node="0>0|0|10" />
		<i3dMapping id="PTObackTractor_decal" node="0>0|0|10|0" />
		<!-- --- -->
	  <!--Numbers-->
        <i3dMapping id="numbersRPM01" node="0>0|1|0" />
        <i3dMapping id="numbersSpeed01" node="0>0|1|1" />
	  <!--sliders-->		
        <i3dMapping id="sliderTemperature" node="0>0|2|0" />
        <i3dMapping id="sliderRPM" node="0>0|2|1" />
        <i3dMapping id="sliderAir" node="0>0|2|2" />
        <i3dMapping id="sliderDef" node="0>0|2|3" />
        <i3dMapping id="sliderSpeed01" node="0>0|2|4" />
        <i3dMapping id="sliderFuel" node="0>0|2|5" />
		
        <i3dMapping id="fendtOneDisplayFront_decal" node="0>1" />
	<!-- FendtOne Display Side -->
	  <!--Icons-->
        <i3dMapping id="fendtOneDisplaySide" node="1>" />
        <i3dMapping id="fendtOneDisplaySideContent" node="1>0" />
		<!--iconCruiseControl-->
        <i3dMapping id="iconDefaultLight02_decal" node="1>0|0|0" />
        <i3dMapping id="iconWorklightFront_decal" node="1>0|0|1" />
		<!--LightGroups-->
        <i3dMapping id="BeaconLight" node="1>0|0|2|0" />
        <i3dMapping id="BackLight#1" node="1>0|0|2|1" />
        <i3dMapping id="BackLight#2" node="1>0|0|2|2" />
        <i3dMapping id="BackLight#3" node="1>0|0|2|3" />	
        <i3dMapping id="FrontLight#1" node="1>0|0|2|4" />
        <i3dMapping id="FrontLight#2" node="1>0|0|2|5" />
        <i3dMapping id="FrontLight#3" node="1>0|0|2|6" />
        <i3dMapping id="FrontLight#4" node="1>0|0|2|7" />
        <i3dMapping id="FrontLight#5" node="1>0|0|2|8" />
        <i3dMapping id="FrontLightMAIN" node="1>0|0|2|9" />
	  <!--numbers-->
        <i3dMapping id="numbersTime01" node="1>0|1|0" />
        <i3dMapping id="numbersSpeed02" node="1>0|1|1" />
        <i3dMapping id="numbersFuelUsage01" node="1>0|1|2" />
        <i3dMapping id="numbersCruiseControl01" node="1>0|1|3" />
        <i3dMapping id="numbersCruiseControl02" node="1>0|1|4" />
        <i3dMapping id="#1numbersCruiseControl03" node="1>0|1|5" />
        <i3dMapping id="#2numbersCruiseControl03" node="1>0|1|6" />
		<i3dMapping id="PTObackNumbers" node="1>0|1|7" />
		<i3dMapping id="PTOfrontNumbers" node="1>0|1|8" />
	  <!--visibles-->
		<i3dMapping id="CruiseControl#1" node="1>0|2|0" />
		<i3dMapping id="CruiseControl#2" node="1>0|2|1" />
		<i3dMapping id="dbl_pto_f" node="1>0|2|2" />
		<i3dMapping id="dbl_pto_b" node="1>0|2|3" />
	  <!--sliders-->				
        <i3dMapping id="sliderSpeed02" node="1>0|3|0" />
		<!--LiftLeft-->
		<i3dMapping id="LiftleftDY" node="1>0|3|1" />	
		<i3dMapping id="LiftleftDYArrow" node="1>0|3|1|0" />
		<!--LiftRight-->
		<i3dMapping id="LiftrightDY" node="1>0|3|2" />
		<i3dMapping id="LiftrightDYArrow" node="1>0|3|2|0" />
			
        <i3dMapping id="fendtOneDisplaySide_decal" node="1>1" />
	<!--FendtOne Display Roof-->
	  <!--icons-->
        <i3dMapping id="fendtOneDisplayRoof" node="2>" />
        <i3dMapping id="fendtOneDisplayRoofContent" node="2>0" />
        <i3dMapping id="iconCruiseControl03_decal" node="2>0|0|0" />
	  <!--numbers-->
        <i3dMapping id="numbersTime02" node="2>0|1|0" />
        <i3dMapping id="numbersFuelUsage02" node="2>0|1|1" />
